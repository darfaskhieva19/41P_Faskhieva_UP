<Window x:Class="УП_Мухина.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:УП_Мухина"
        mc:Ignorable="d"
        Title="Пиши-стирай" Height="450" Width="800" Icon="/resource/icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1.5*"/>
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <Image Source="/resource/logo.png" Width="65"/>
            <TextBlock Text="Магазин канцелярии *Пиши-стирай*" Name="tbHeader" Style="{StaticResource txtHeader}"/>          
        </StackPanel>
        <Frame Grid.Row="1" Name="Mframe" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
